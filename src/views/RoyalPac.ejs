<!DOCTYPE html>
 <html>
   <head>
     <meta charset="utf-8">
     <title>RoyalPac</title>
     <script src="phaser/phaser.min.js"></script>
     <script type="text/javascript" src="/script/RoyalPac.js"></script>
     <script type="text/javascript" src="/script/fantome.js"></script>
     <script type="text/javascript" src="/script/point.js"></script>
     <script type="text/javascript" src="/script/pathfinding.js"></script>
     <script type="text/javascript" src="/scriptClient/terminal.js"></script>
     <script src="/socket.io/socket.io.js"></script>
     <link href="/css/style.css" rel="stylesheet" type="text/css" media="all">
   </head>
   <body>
     <script type="text/javascript">
       var socket = io.connect('http://localhost:8080');



       socket.on('message', function(message){
         var terminal = document.getElementById('terminal');
         var newMess = document.createElement('p');
         var d = new Date();


         //alert("message du serveur : " + message.content)
         newMess.innerHTML = d.getDate()+"/"+d.getDay()+"/"+ d.getFullYear() + " : " + message.content;

         terminal.appendChild(newMess);
       });
     </script>
     <div id="terminal">
      <input class="commandLine" type="text" name="commandLine" value="" onkeypress="return runScript(event)">
      <center>  terminal </center>

     </div>
   </body>
 </html>
